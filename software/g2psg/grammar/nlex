{Noun lexicon}

{Common nouns - plural take affix, singular take base form}
N0[PLU -, POSS -]: Noun[H]
N0[PLU +, MORPH S, POSS -]: Noun[H], pluaffix
pluaffix: "s"

N0[POSS +]: Noun[H], posaffix[H]

{Possessive affixes}
posaffix[POSS +, PLU -]: "'s"
posaffix[POSS +, PLU +, MORPH S]: "'"
posaffix[POSS +, PLU +, MORPH C]: "'s"

{Unquantified nouns}
N2[CONJ NIL, PRO -, RE -, PLU -, PER 3, NOM -, POSS -]: "ear"
       {by ear}

{Pronouns}
N2[PRO +, RE -, PLU -, PER 1, NOM +, POSS -]: "I"
N2[PRO +, RE -, PLU -, PER 1, NOM -, POSS -]: "me"
N2[PRO +, RE -, PER 2, POSS -, NFORM NORM]: "you"
N2[PRO +, RE -, PLU -, PER 3, GEN F, NOM +, POSS -]: "she"
N2[PRO +, RE -, PLU -, PER 3, GEN F, NOM -, POSS -]: "her"
N2[PRO +, RE -, PLU -, PER 3, GEN M, NOM +, POSS -]: "he"
N2[PRO +, RE -, PLU -, PER 3, GEN M, NOM -, POSS -]: "him"
N2[PRO +, RE -, PLU +, PER 1, NOM +, POSS -]: "we"
N2[PRO +, RE -, PLU +, PER 1, NOM -, POSS -]: "us"
N2[PRO +, RE -, PLU +, PER 3, NOM +, POSS -]: "they"
N2[PRO +, RE -, PLU +, PER 3, NOM -, POSS -]: "them"
N2[PRO +, RE -, PLU -, PER 3, POSS -, NFORM NORM]: "it"
N2[PRO +, RE -, PLU -, PER 3, POSS -]: "this"
N2[PRO +, PLU -, PER 3, POSS -]: "that"

{Possessive pronouns}
N2[PRO +, RE -, PLU -, PER 1, POSS +]: "my"
N2[PRO +, RE -, PLU -, PER 3, POSS -]: "mine"
N2[PRO +, RE -, PER 2, POSS +]: "your"
N2[PRO +, RE -, PER 3, POSS -]: "yours"
N2[PRO +, RE -, PLU -, PER 3, GEN F, POSS +]: "her"
N2[PRO +, RE -, PLU -, PER 3, POSS -]: "hers"
N2[PRO +, RE -, PLU -, PER 3, GEN M, POSS +]: "his"
N2[PRO +, RE -, PLU -, PER 3, POSS -]: "his"
N2[PRO +, RE -, PLU +, PER 1, POSS +]: "our"
N2[PRO +, RE -, PLU +, PER 3, POSS -]: "ours"
N2[PRO +, RE -, PLU +, PER 3, POSS +]: "their"
N2[PRO +, RE -, PLU +, PER 3, POSS -]: "theirs"

{Reflexive pronouns}
N2[PRO +, RE +, PLU -, PER 1, POSS -, NOM -]: "myself"
N2[PRO +, RE +, PLU -, PER 2, POSS -, NOM -]: "yourself"
N2[PRO +, RE +, PLU -, PER 3, GEN F, POSS -, NOM -]: "herself"
N2[PRO +, RE +, PLU -, PER 3, GEN M, POSS -, NOM -]: "himself"
N2[PRO +, RE +, PLU +, PER 1, POSS -, NOM -]: "ourselves"
N2[PRO +, RE +, PLU +, PER 2, POSS -, NOM -]: "yourselves"
N2[PRO +, RE +, PLU +, PER 3, POSS -, NOM -]: "themselves"

{NFORM it, there}
N2[NFORM it, RE -, POSS -]: "it"
N2[NFORM there, RE -, POSS -]: "there"

{Proper nouns}
N2[CONJ NIL, PRO -, RE -, PLU -, PER 3, GEN F]: "Ruth"
N2[CONJ NIL, PRO -, RE -, PLU -, PER 3, GEN F]: "Carol"
N2[CONJ NIL, PRO -, RE -, PLU -, PER 3, GEN M]: "Stephen"
N2[CONJ NIL, PRO -, RE -, PLU -, PER 3, GEN M]: "Dave"
N2[CONJ NIL, PRO -, RE -, PLU -, PER 3, GEN M]: "Charles"
N2[CONJ NIL, PRO -, RE -, PLU -, PER 3, GEN M]: "Paul"
N2[CONJ NIL, PRO -, RE -, PLU -, PER 3, GEN M]: "Mick"
N2[CONJ NIL, PRO -, RE -, PLU -, PER 3, GEN M]: "John Birch"
N2[CONJ NIL, PRO -, RE -, PLU -, PER 3, GEN M]: "Mick Chandler"
N2[CONJ NIL, PRO -, RE -, PLU -, PER 3, GEN N]: "Chelmsford"
N2[CONJ NIL, PRO -, RE -, PLU -, PER 3, GEN N]: "Baddow"
N2[CONJ NIL, PRO -, RE -, PLU -, PER 3, GEN N]: "Great Baddow"
N2[CONJ NIL, PRO -, RE -, PER 3, GEN N]: "Marconi Great Baddow"
N2[CONJ NIL, PRO -, RE -, PER 3]: "Hurst Research"
N2[CONJ NIL, PRO -, RE -, PER 3]: "GEC Hurst Research"
N2[CONJ NIL, PRO -, RE -, PER 3]: "TAB"
N2[CONJ NIL, PRO -, RE -, PER 3]: "GEC"
N2[CONJ NIL, PRO -, RE -, PER 3]: "Marconi"
N2[CONJ NIL, PRO -, RE -, PER 3]: "Marconi Research"

{Non-count nouns}
N1[RE -, PLU -]: "involvement"
